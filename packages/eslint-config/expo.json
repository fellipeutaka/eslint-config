{
  "$schema": "https://json.schemastore.org/eslintrc",
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "./base",
    "plugin:react/jsx-runtime",
    "plugin:react-hooks/recommended",
    "plugin:react-native-a11y/all"
  ],
  "plugins": ["react"],
  "overrides": [
    {
      "files": ["App.tsx"],
      "rules": {
        "import/no-default-export": "off"
      }
    }
  ]
}
